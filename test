#!/usr/bin/python
from sys import argv
import algorithms.one_dimentional as od

def default():
    print('...nothing')

def first_lab():
    f = lambda x: x**2+2*x-4
    a = -2
    b = 1

    print(f'fibonacci: {round(od.fibonacci(f, a, b), 2)}')
    print(f'dichotomy: {round(od.dichotomy(f, a, b), 2)}')

def second_lab():
    pass

funcs = {
    '1': first_lab,
    '2': second_lab,
}

argv.append('')
funcs.get(argv[1], default)()

